{
  "name": "Comet",
  "language": ["en"],
  "domains": ["comet.elfhosted.com"],
  "base_url": "https://comet.elfhosted.com",
  "search_url_format_movie": {
    "string_format": "%s.json",
    "replacement": ["imdb_id"]
  },
  "search_url_format_episode": {
    "string_format": "%s:%d:%d.json",
    "replacement": ["imdb_id", "season_int", "episode_int"]
  },
  "search_url_format_season_pack": {
    "string_format": "%s:%d:1.json",
    "replacement": ["imdb_id", "season_int"]
  },
  "api": {
    "path_to_array": "streams",
    "path_title": "behaviorHints.filename",
    "path_size": "behaviorHints.videoSize",
    "path_hash": "infoHash"
  },
  "is_torrent": true,
  "is_direct": false
}
