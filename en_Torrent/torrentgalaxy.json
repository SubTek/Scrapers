{
  "name": "torrentgalaxy",
  "language": ["en"],
  "domains": ["torrentgalaxy.one", "torrentgalaxy.space", "torrentgalaxy.info", "torrentgalaxy.unblockedfor.me"],
  "base_url": "https://torrentgalaxy.one",

  "search_url_format_episode": {
    "string_format": "/get-posts/keywords:%s S%02dE%02d:format:json/",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/get-posts/keywords:%s %04d:format:json/",
    "replacement": [
      "title_lower_or_imdb",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/get-posts/keywords:%s Season:format:json/",
    "replacement": [
      "title_lower"
    ]
  },

  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": true,
  "drill_down_path": "",
  "drill_down_path_attr": "",

  "is_torrent": true,
  "is_direct": false,

  "links_container_path": "",
  "link_path": "",
  "link_path_attr": "",

  "title_path": "",
  "title_path_attr": "",
  "details_path": "",
  "details_path_attr": "",

  "magnet_path": "",
  "magnet_path_attr": "",

  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "",
  "filesize_path_attr": "",
  "quality_path": "",
  "quality_path_attr": "",
  "info_path": "",
  "info_path_attr": "",

  "api_is_json": true,
  "json_fields": {
    "title": "n",
    "hash": "h",
    "seeders": "se",
    "size_bytes": "s"
  },
  "construct_magnet_from_hash": true,
  "debrid_only": true,
  "min_seeders": 0
}
